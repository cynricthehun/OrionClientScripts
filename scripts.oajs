function Hiding() {
    	while (Orion.SkillValue('hiding') < 1000){
       	Orion.UseSkill('Hiding');
        	Orion.Wait(3400);
    	}
    	while (Orion.SkillValue('Spirit Speak') < 1000){
       	Orion.UseSkill('Spirit Speak');
        	Orion.Wait(3400);    		
    	}
}
function Wrestling() {
	while (Orion.SkillValue('Wrestling') < 1000){
       	void Orion.UseObject(['0x403BD4FC']);
       	void Orion.UseObject(['0x403BD53E']);
        	Orion.Wait(2600);	
    	}
}